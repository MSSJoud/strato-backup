    "from shapely.wkt import loads\n",
    "from shapely.ops import transform\n",
    "from pyproj import Transformer\n",
    "\n",
    "# Your AOI in WGS84\n",
    "aoi_wkt = \"POLYGON((11.2 44.4, 11.5 44.4, 11.5 44.6, 11.2 44.6, 11.2 44.4))\"\n",
    "aoi_geom = loads(aoi_wkt)\n",
    "\n",
    "# Reproject from EPSG:4326 to EPSG:32633\n",
    "project = Transformer.from_crs(\"EPSG:4326\", \"EPSG:32633\", always_xy=True).transform\n",
    "aoi_utm = transform(project, aoi_geom)\n",
    "\n",
    "# Get projected bounding box\n",
    "minx, miny, maxx, maxy = aoi_utm.bounds\n",
    "print(f\"AOI in UTM 33N (EPSG:32633): [ulx={minx}, uly={maxy}, lrx={maxx}, lry={miny}]\")\n"
   ]
  },
